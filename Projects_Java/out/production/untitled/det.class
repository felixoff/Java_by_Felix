import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.Arrays;
import java.util.stream.IntStream;


public class det {
    public static void main(String[] args) throws IOException {
        BufferedReader inp = new BufferedReader(new InputStreamReader(System.in));
        String[] mas0 = inp.readLine();
        System.out.println(mas0);
        char[] mas = new String.toCharArray(mas0);
        System.out.println(mas.toString());
        long arab = 0;
        for (int i = 0; i < mas.length - 1; i++) {
            if(mas[i] == 'I' && mas[i + 1] != 'V' && mas[i+1] != 'X')
                arab += 1;
            else if (mas[i] == 'X' && mas[i + 1] != 'L' && mas[i+1] != 'C')
                arab += 10;
            else if (mas[i] == 'C' && mas[i + 1] != 'D' && mas[i+1] != 'M')
                arab += 100;
            else if (mas[i] == 'D')
                arab += 500;
            else if (mas[i] == 'M')
                arab += 1000;
            else if (mas[i] == 'I' && mas[i + 1] == 'V')
            {
                arab += 4;
                i++;
            }
            else if (mas[i] == 'I' && mas[i + 1] == 'X')
            {
                arab += 9;
                i++;
            }
            else if (mas[i] == 'X' && mas[i + 1] == 'L')
            {
                arab += 40;
                i++;
            }
            else if (mas[i] == 'X' && mas[i + 1] == 'C')
            {
                arab += 90;
                i++;
            }
            else if (mas[i] == 'C' && mas[i + 1] == 'D')
            {
                arab += 400;
                i++;
            }
            else if (mas[i] == 'C' && mas[i + 1] == 'M')
            {
                arab += 900;
                i++;
            }
        }
    }
}
